%table
  %thead
  %tr
    %th First
    %th Last
    %th Email
  %thead
  %tbody
    contacts.each do |contact|
      %tr
        %td= contact.first
        %td= contact.last
        %td= contact.phone
        %td= contact.email
        %td
          %a{href: "/contacts/#{contact.id}/edit"} Edit
          %a{href: "/contacts/#{contact.id}"} View
    end
